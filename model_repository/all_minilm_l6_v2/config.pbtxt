name: "all_minilm_l6_v2"
backend: "python"
max_batch_size: 8
input [
  {
    name: "INPUT"
    data_type: TYPE_STRING
    dims: [ -1 ]
  }
]
output [
  {
    name: "OUTPUT"
    data_type: TYPE_FP32
    # dims: [batch, embedding_dim]; use -1 for dynamic second dim
    dims: [ -1, 384 ]
  }
]
instance_group [
  {
    kind: KIND_CPU
    count: 1
  }
]
